#!/usr/bin/env ruby

require 'bundler/setup'
require 'pry'
require 'transform_tree'

tree = TransformTree::Root.new
tree.add_transforms(
  TransformTree::Transforms::ret('woof'),
  TransformTree::Transforms::ret('arf')
)
tree.add_transforms(
  ->(str) { str.upcase },
  ->(str) { str.upcase },
  TransformTree::Transforms::null
)

Pry.start
